{
  "_id": "static",
  "startUrls": "https://demo-site.at.ispras.ru/static",
  "beginProgram": [1, 2],
  "selectors": [
    {
      "id": "1",
      "type": "goto",
      "link": "S:startUrls"
    },
    {
      "id": "2",
      "type": "foreach",
      "subType": "selector",
      "selector": ".nav ul li a",
      "selectorName": "S:sel1",
      "program": [3, 4, 5, 6]
    },
    {
      "id": "3",
      "type": "extractor",
      "subType": "url",
      "selector": "S:sel1",
      "name": "S:link1",
      "multiple": false
    },
    {
      "id": "4",
      "type": "goto",
      "link": "S:link1"
    },
    {
      "id": "5",
      "type": "constant",
      "name": "S:page",
      "value": "0"
    },
    {
      "id": "6",
      "type": "while",
      "firstOperand": "S:page",
      "logicalOperation": "<=",
      "secondOperand": "3",
      "program": [7, 8, 9]
    },
    {
      "id": "7",
      "type": "foreach",
      "subType": "selector",
      "selector": "h5.card-title a",
      "selectorName": "S:sel2",
      "program": [10, 11, 12]
    },
    {
      "id": "8",
      "type": "click",
      "selector": ".btn"
    },
    {
      "id": "9",
      "type": "increment",
      "name": "page"
    },
    {
      "id": "10",
      "type": "extractor",
      "subType": "url",
      "selector": "S:sel2",
      "name": "S:link2",
      "multiple": false
    },
    {
      "id": "11",
      "type": "goto",
      "link": "S:link2"
    },
    {
      "id": "12",
      "type": "include",
      "label": "someScrapeMap"
    }
  ]
}

