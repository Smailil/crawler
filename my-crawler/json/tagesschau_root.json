{
  "_id": "tagesschau_root",
  "startUrls": ["https://www.tagesschau.de/archiv/"],
  "beginProgram": ["1", "2"],
  "selectors": [
    {
      "id": "1",
      "type": "extractor",
      "subType": "html",
      "selector": "li.columns > div > div",
      "name": "S:elements",
      "multiple": true
    },
    {
      "id": "2",
      "type": "foreach",
      "array": "S:elements",
      "name": "S:element",
      "program": ["3", "4"]
    },
    {
      "id": "3",
      "type": "fromHTMLExtractor",
      "subType": "text",
      "html": "S:element",
      "selector": "a > p.teaser-xs__topline-container > span.teaser-xs__date",
      "name": "S:date",
      "multiply": false
    },
    {
      "id": "4",
      "type": "branchCondition",
      "firstOperand": "S:date",
      "logicalOperation": "includes",
      "secondOperand": "17.03.2023",
      "ifProgram": ["5", "6", "7"],
      "elseProgram": []
    },
    {
      "id": "5",
      "type": "fromHTMLExtractor",
      "subType": "url",
      "html": "S:element",
      "selector": "a",
      "name": "S:url",
      "multiply": false
    },
    {
      "id": "6",
      "type": "goto",
      "link": "S:url"
    },
    {
      "id": "7",
      "type": "include",
      "label": "tagesschau_scrapemap"
    }
  ]
}